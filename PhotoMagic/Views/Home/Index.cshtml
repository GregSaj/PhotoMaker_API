@{
    ViewData["Title"] = "Home Page";
}

<div class="container text-center">
    <div class="row">
        <img src="/Content/Images/Logo.jpg" />
    </div>
    <div class="row">
        <p>PhotoMAGIC </p>
        <p>Podkręć swoje zdjęcia mocą AI</p>
    </div>
    <div class="row mb-5">
        <form asp-controller="PhotoMagic" asp-action="Welcome" method="post">
            <div class="d-grid gap-2">
                <button class="btn btn-primary rounded-3" type="submit">Zaczynamy!</button>
            </div>
        </form>
        <form asp-controller="Users" asp-action="Index" method="post">
            <div class="d-grid gap-2">
                <button class="btn btn-primary rounded-3" type="submit">Users</button>
            </div>
        </form>
    </div>
</div>
